include "system/public/magenta/mdi/magenta.mdi"

cpu-map = {
    clusters = {
        {
            cpu-count = 4
        }
    }
}

kernel = {
    s905-uart = {
        portnum = 3
    }

    arm-gic-v2 = {
        base-virt = 0xffffffffc4300000
        base-phys = 0xc4300000
        gicc-offset = 0x002000
        gicd-offset = 0x001000
        ipi-base = 5
    }
    arm-psci = {
        use-smc = true
    }
    arm-timer = {
        irq-phys = 30 // INTERRUPT_ARM_LOCAL_CNTVIRQ
    }
}
